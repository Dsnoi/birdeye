<?xml version="1.0" encoding="utf-8"?>
<mx:VBox
	xmlns:mx="http://www.adobe.com/2006/mxml" 
	xmlns:gv="views.graphs.*" 
	xmlns:layouter="birdeye.vis.trans.graphs.layout.*"
	creationComplete="graphView.generateGraph()"
	initialize="init()"
	>

	<gv:BasicGraphView id="graphView" width="100%" height="100%">
		<gv:layouter>
			<layouter:CircularLayouter />
		</gv:layouter>	
	</gv:BasicGraphView>

	<mx:Script>
	<![CDATA[
		private function init():void { 
			graphView.generateGraphButton.enabled = true;
		}
	]]>
	</mx:Script>

</mx:VBox>
