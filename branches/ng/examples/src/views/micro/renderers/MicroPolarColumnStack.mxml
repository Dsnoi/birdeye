<?xml version="1.0" encoding="utf-8"?>
<mx:HBox xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:mc="org.un.cava.birdeye.qavis.microcharts.*" paddingTop="0"
	horizontalAlign="center" width="100%" height="100%"
	xmlns:coords="birdeye.vis.coords.*"
	xmlns:elements="birdeye.vis.elements.geometry.*"
	xmlns:scales="birdeye.vis.scales.*">

	<coords:Polar width="100%" height="100%" dataProvider="{data.dataProvider}" showDataTips="true" type="stacked100">
		<coords:scales>
			<scales:CategoryAngle id="c6" categoryField="subject" />
			<scales:Linear id="l6" />
		</coords:scales>
		<coords:elements>
			<elements:ColumnElement scale1="{c6}" scale2="{l6}" baseAt="{NaN}" extendMouseEvents="true" styleName="grad3" dim1="subject" dim2="R3"/>
			<elements:ColumnElement scale1="{c6}" scale2="{l6}" baseAt="{NaN}" extendMouseEvents="true" styleName="grad1" dim1="subject" dim2="R2"/>
		</coords:elements>
	</coords:Polar>
</mx:HBox>
