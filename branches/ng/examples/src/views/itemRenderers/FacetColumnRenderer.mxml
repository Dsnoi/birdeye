<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" 
	xmlns:elements="birdeye.vis.elements.geometry.*"
	xmlns:scales="birdeye.vis.scales.*"
	xmlns:legend="birdeye.vis.guides.legend.*"
	xmlns:grid="birdeye.vis.guides.grid.*"
	backgroundAlpha="0"
	xmlns:coords="birdeye.vis.coords.*" xmlns:axis="birdeye.vis.guides.axis.*" xmlns:cartesian="views.cartesian.*">

	<mx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
						
			[Bindable]
			public var dt:ArrayCollection;;

			private var subject:Array;
			
			override public function set data(value:Object):void
			{
				super.data = value;
				dt = ArrayCollection(data);
			} 
		]]>
	</mx:Script>
 	<coords:Cartesian showDataTips="true" collisionType="stacked100" dataProvider="{dt}" width="100%" height="100%" columnWidthRate="0.5">
	 	<coords:scales>
	 	 	<scales:Category id="c" categoryField="pressure" />
		 	<scales:Linear id="l"  numberOfIntervals="1"/>       		 				
	 	</coords:scales>
	 	<coords:guides>
	 		<axis:Axis scale="{c}" placement="bottom" sizeLabel="9" />
	 		<axis:Axis scale="{l}" placement="left" sizeLabel="9" />	
	 	</coords:guides>
	 	<coords:elements>
	 		<elements:ColumnElement id="a1" scale1="{c}" scale2="{l}" showAllDataItems="true" dim1="pressure" dim2="R1" styleName="solid1" displayName="R1" />
		 	<elements:ColumnElement id="a2" scale1="{c}" scale2="{l}" showAllDataItems="true" dim1="pressure" dim2="R2" styleName="solid2" displayName="R2" />
	 	</coords:elements>
	 </coords:Cartesian>
</mx:VBox>
